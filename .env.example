# Convex (see .env.local for your deployment)
# CONVEX_DEPLOYMENT=
# VITE_CONVEX_URL=

# LinkedIn import pipeline (used by Convex actions)
# Set in Convex Dashboard → Settings → Environment Variables for production.
# For local dev, add to .env.local so `npx convex dev` can run the import action.
LINKDAPI_API_KEY=
LINKEDIN_PROVIDER=linkdapi

# AI normalization for search arrays (optional). Set LLM_PROVIDER=claude or gemini.
# Omit or set to "off" to skip normalization and use raw arrays.
LLM_PROVIDER=off
ANTHROPIC_API_KEY=
GEMINI_API_KEY=
